</body><script>
 const __modal = document.getElementById("modal68c043f5d8138a07d7999e5c");
    const __iframe = document.getElementById("iframe68c043f5d8138a07d7999e5c");
    const __domainInput = document.getElementById("domainInput68c043f5d8138a07d7999e5c");
    const __partnerId = "68c043f5d8138a07d7999e5c";
    const __aceUrl = "https://acsbace.com";

    __domainInput.addEventListener("input", function (event) {
      __domainInput.setCustomValidity("");
    });

    function submitNameAndEmail(guid) {
        const email = document.getElementById("email68c043f5d8138a07d7999e5c")?.value;
        const name = document.getElementById("fullName68c043f5d8138a07d7999e5c")?.value;
        const domain= document.getElementById("domainInput68c043f5d8138a07d7999e5c")?.value;

        const postData = { email, name, domain, guid };
          
          fetch(`${__aceUrl}/api/partners/lead-email`, {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
              'x-api-key': __partnerId
            },
            body: JSON.stringify(postData)
          })
        .then(data => {})
        .catch(error => {});
    }

    function submitform(event) {
        const domainInput = document.getElementById("domainInput68c043f5d8138a07d7999e5c");
        const domain = domainInput.value;
        if (isValidDomain(domain)) {
            domainInput.setCustomValidity("");
            const u = Date.now().toString(16) + Math.random().toString(16) + '0'.repeat(16);
            const guid = [u.substr(0,8), u.substr(8,4), '4000-8' + u.substr(13,3), u.substr(16,12)].join('-');
            submitNameAndEmail(guid);
            openPopupWithIframe(domain, guid);
        } else {
            domainInput.setCustomValidity("Please enter a valid url");
            domainInput.reportValidity();
        }
        event.preventDefault();
        return false;
    }

    function closeButtonClickHandler(e) {
        e.target.removeEventListener("click", closeButtonClickHandler)
        __iframe.src = '';
        __modal.style.display = "none";
    }
    
    function isValidDomain(domain) {
        const urlRegex =/^(?:(?:https?|ftp):\/\/)?(?:www\.)?[a-zA-Z0-9-]+\.[a-zA-Z0-9]+(?:\.[a-zA-Z0-9]+)*(?:\/[^\s]*)?$/;
        return urlRegex.test(domain);
    }
    function getLeadCloseColor(e) { let { ratioWithBlack: t, ratioWithWhite: $ } = getBlackAndWhiteRatio(e); return $ > t ? "ffffff" : "000000" } function getRatio(e, t) { let $ = getLuminance(e.r, e.g, e.b), n = getLuminance(t.r, t.g, t.b); return (Math.max($, n) + .05) / (Math.min($, n) + .05) } function getLuminance(e, t, $) { let n = [e, t, $].map(e => (e /= 255) <= .03928 ? e / 12.92 : Math.pow((e + .055) / 1.055, 2.4)); return .2126 * n[0] + .7152 * n[1] + .0722 * n[2] } function getBlackAndWhiteRatio(e) { if (!e) return {}; e = e.replace(/^#/, "").toLowerCase(); let { r: t, g: $, b: n } = hexToRgb(e), a = getRatio({ r: t, g: $, b: n }, { r: 255, g: 255, b: 255 }), r = getRatio({ r: t, g: $, b: n }, { r: 0, g: 0, b: 0 }); return { ratioWithBlack: r, ratioWithWhite: a } } function hexToRgb(e) { let [t, $, n] = e.match(/([a-fA-F0-9]{2})([a-fA-F0-9]{2})([a-fA-F0-9]{2})/).slice(1).map(e => parseInt(e, 16)); return { r: t, g: $, b: n } }

    function openPopupWithIframe(url, guid) {
        __modal.style.display = "block";
        const leadCloseColor = getLeadCloseColor("#0A2540");
        const prefix = `${__aceUrl}?sessionid=${guid}&partnerid=68c043f5d8138a07d7999e5c#audit`;
        __modal.style.setProperty("--close-button-color", "#" + leadCloseColor);
        __iframe.src = `${prefix}/${url}`;
        const closeButton = document.getElementById("closeButton68c043f5d8138a07d7999e5c");
        closeButton.addEventListener("click", closeButtonClickHandler);
    }
</script>
